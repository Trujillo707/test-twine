:: StoryStylesheet [stylesheet]
/* Making Regular Text look like button */
.blueTextBox {
  color: #eee;
  background-color: #35a;
  border: 1px solid #57c;
  padding: 0.4em;
  line-height:1.75;
  display:inline-block;
}

#passageHeader {
  text-align: right;
}
#passageHeader hr {
 margin-bottom: 0;
}

#randPopUp {
  position: fixed;
  bottom: 10px;
  right: 15px;
}

.invBody{
 background-image: url("https://img.freepik.com/free-vector/shine-old-wooden-chest-realistic-composition-treasure-coffer-with-open-lid-golden-particles_1284-33068.jpg?t=st=1740962116~exp=1740965716~hmac=d9188c88f1cfa489ffba84f4a01c994eb16751e51ebd0c15cea5e937201fcb39&w=740");
 background-position: center;
 background-size: auto;
 background-repeat: no-repeat;
  background-color: black;
 color: white;
  min-height: 700px;
  position: relative;
}

.iconButton{
  max-width: 10rem;
  max-height: 10rem;
  padding: 0;
  background-color: transparent;
}

.iconButton *{
    background-color: transparent;
  border: none;
}

.invBody #goBack {
  position: absolute;
  left: 0;
  bottom: 0;
}

.invBody button img{
  object-fit: contain;
  width: 10rem;
  height: 10rem;
}

.center {
 text-align: center; 
}


.mapContainer{
  display: flex;
  justify-content: center;
}

.map{
  transform: scale(0.5);
  transform-origin: top center;
}
